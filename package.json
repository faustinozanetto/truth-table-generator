    "@typescript-eslint/parser": "5.57.0",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.36.0",
    "eslint-define-config": "1.17.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "postcss": "8.4.38",
    "prettier": "2.8.7",
